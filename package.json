    "express": "^4.17.1",
    "mariadb": "^2.5.4",
    "sequelize": "^6.6.5",
    "socket.io": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.2.37",
    "pg": "^8.7.1",
    "bcryptjs": "^2.4.3",
    "reflect-metadata": "^0.1.13",
    "express-async-errors": "^3.1.1",
